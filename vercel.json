{
    "version": 2,
    "builds": [
      {
        "src": "auth_project/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.12.3",
          "excludeFiles": ["requirements.txt"]
        }
      },
      {
        "src": "build.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles_build"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "auth_project/wsgi.py"
      }
    ],
    "externalPackages": ["numpy", "pandas"] 
  }
  